{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "max-len": [
      1,
      120,
      2
    ],
    "react/prop-types": "off",
    "camelcase": "warn"
  }
}
